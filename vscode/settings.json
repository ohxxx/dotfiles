{
  /*****************************************
  *                                        *
  *                Workbench               *
  *                                        *
  *****************************************/
  "tabnine.experimentalAutoImports": true,
  // I changed the theme color
  "workbench.iconTheme": "gruvbox-material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.colorCustomizations": {
    "[Panda Syntax]": {
      "activityBar.background": "#292A2B",
      "activityBar.border": "#292A2B",
      "sideBar.background": "#292A2B",
      "sideBar.border": "#484849",
      "statusBar.background": "#292A2B",
      "statusBar.border": "#484849",
      "titleBar.activeBackground": "#292A2B",
      "activityBar.activeBorder": "#19f9d8",
      "tab.inactiveBackground": "#292A2B",
      "tab.border": "#292A2B",
      "list.inactiveSelectionBackground": "#484849",
      "editor.background": "#292A2B",
      "editorGroupHeader.tabsBackground": "#292A2B",
      "editorGroupHeader.border": "#484849",
      "editorError.border": "#FF4B8200",
      "editorWarning.border": "#FF4B8200",
      "editorInfo.border": "#FF4B8200"
    }
  },
  "workbench.editorAssociations": {
    "*.sqlite": "default",
    "*.ttf": "default"
  },
  "window.autoDetectColorScheme": true,
  "window.commandCenter": true,
  "workbench.colorTheme": "Vitesse Dark Soft",
  "workbench.preferredLightColorTheme": "Vitesse Light",
  "workbench.preferredDarkColorTheme": "Vitesse Dark Soft",
  "files.autoSave": "off",
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "newWindow",
  /*****************************************
  *                                        *
  *                   Git                  *
  *                                        *
  *****************************************/
  "git.ignoreMissingGitWarning": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  /*****************************************
  *                                        *
  *                 ESLint                 *
  *                                        *
  *****************************************/
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.quiet": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  /*****************************************
  *                                        *
  *                 Editor                 *
  *                                        *
  *****************************************/
  "prettier.enable": false,
  "editor.columnSelection": false,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.suggestSelection": "first",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.accessibilitySupport": "off",
  "editor.tabSize": 2,
  /*****************************************
  *                                        *
  *                Terminal                *
  *                                        *
  *****************************************/
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  /*****************************************
  *                                        *
  *                Languages               *
  *                                        *
  *****************************************/
  // [ runner ]
  "jestrunner.jestPath": "node_modules/jest/bin/jest.js",
  "code-runner.runInTerminal": true,
  // [ javascript ]
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "javascript.validate.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  // [ typescript ]
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  // [ json ]
  "[json]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  // [ html ]
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "emmet.includeLanguages": {
    "wxml": "html"
  },
  "dart.debugSdkLibraries": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.suggest.insertMode": "replace"
  },
  "dart.checkForSdkUpdates": false,
  "files.associations": {
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript"
  },
  /*****************************************
  *                                        *
  *              File Nesting              *
  *                                        *
  *****************************************/
  // https://github.com/antfu/vscode-file-nesting-config
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": { /** ... */},
  /*****************************************
  *                                        *
  *            Extensions Config           *
  *                                        *
  *****************************************/
  // [ Settings Sync ]
  "sync.gist": "xxx",
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  // [ CommentTranslate ]
  "commentTranslate.targetLanguage": "zh-CN",
  // [ Jest ]
  "jest.autoRun": "off",
  // [ Layered Comments ]
  "layeredComments.style": {
    "width": 42
  },
  // [ i18n Ally ]
  "i18n-ally.autoDetection": false,
  "i18n-ally.displayLanguage": "zh-CN",
  // [ Iconify IntelliSense ]
  "iconify.annotations": true,
  "iconify.inplace": true,
  // [ File Nesting Config ]
  "fileNestingUpdater.autoUpdate": true,
  "fileNestingUpdater.autoUpdateInterval": 720,
  "fileNestingUpdater.promptOnAutoUpdate": true,
  "fileNestingUpdater.upstreamRepo": "antfu/vscode-file-nesting-config",
  "fileNestingUpdater.upstreamBranch": "main",
  // [ Error Lens ]
  "errorLens.enabledDiagnosticLevels": [
    "warning",
    "error",
    "hint"
  ],
  "errorLens.excludeBySource": [
    "cSpell",
    "Grammarly",
    "eslint"
  ],
  // [ Vite ]
  "vite.autoStart": false,
  // [ cSpell ]
  "cSpell.allowCompoundWords": true,
  "cSpell.language": "en,en-US",
  "cSpell.userWords": [
    "antfu",
    "vite",
    "vitejs",
    "vitest",
    "tsup",
    "esbuild",
    "antd",
    "ciri",
    "storejs",
    "ahooks",
    "pnpm",
    "unocss",
    "foca",
    "iconify",
    "nuxt",
    "cascader"
  ],
  // [ Copilot ]
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
  },
}